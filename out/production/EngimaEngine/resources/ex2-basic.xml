<?xml version="1.0" encoding="UTF-8"?>
<CTE-Enigma xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Enigma-Ex2.xsd">
	<CTE-Machine rotors-count="3">
		<ABC>ABCD</ABC>
		<CTE-Rotors>
			<CTE-Rotor id="1" notch="4">
				<CTE-Positioning left="A" right="D"/>
				<CTE-Positioning left="B" right="C"/>
				<CTE-Positioning left="C" right="B"/>
				<CTE-Positioning left="D" right="A"/>
			</CTE-Rotor>
			<CTE-Rotor id="3" notch="1">
				<CTE-Positioning left="A" right="B"/>
				<CTE-Positioning left="B" right="A"/>
				<CTE-Positioning left="C" right="D"/>
				<CTE-Positioning left="D" right="C"/>

			</CTE-Rotor>
			<CTE-Rotor id="2" notch="3">
				<CTE-Positioning left="A" right="D"/>
				<CTE-Positioning left="B" right="C"/>
				<CTE-Positioning left="C" right="B"/>
				<CTE-Positioning left="D" right="A"/>
				</CTE-Rotor>
			</CTE-Rotors>
		<CTE-Reflectors>
			<CTE-Reflector id="I">
				<CTE-Reflect input="1" output="3"/>
				<CTE-Reflect input="2" output="4"/>

					</CTE-Reflector>
				</CTE-Reflectors>
	</CTE-Machine>
	<CTE-Decipher agents="50">
		<CTE-Dictionary exclude-chars="!'#?">
		<Words>
			ABCD
			</Words>
		</CTE-Dictionary>
	</CTE-Decipher>
</CTE-Enigma>